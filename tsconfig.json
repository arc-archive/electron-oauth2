{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": ["es2017", "DOM"],
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "esModuleInterop": true,
    "typeRoots": ["node_modules/@types"]
  },
  "include": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "exclude": []
}
